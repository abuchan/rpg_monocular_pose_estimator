<launch>
  <node name="camera_0" pkg="usb_cam" type="usb_cam_node" output="screen" >
    <param name="video_device" value="/dev/video1" />
    <param name="image_width" value="640" />
    <param name="image_height" value="480" />
    <param name="pixel_format" value="yuyv" />
    <param name="camera_frame_id" value="robot_00/camera_0" />
    <param name="camera_name" value="robot_00/camera_0" />
    <param name="io_method" value="mmap"/>
    <param name="camera_info_url" value="package://monocular_pose_estimator/calib/lifecam.yaml" />
  </node>
</launch>
